	<ThingDef Abstract="True" Name="DoorMatLinkedBase" ParentName="DoorMatBase">
		<graphicData>
			<texPath>Things/Building/Linked/doormatLinked</texPath>
			<graphicClass>Graphic_Single</graphicClass>
